//REXXLAB EXEC PGM=IKJEFT01,                                            00040000
//             PARM='REXXTRY'                                           00051000
//SYSTSPRT  DD SYSOUT=*                                                 00050000
//SYSEXEC   DD DISP=SHR,DSN=TSOXA13.ES52.EXEC                           00060000
//          DD DISP=SHR,DSN=D80WW.ES52V5.RESOURCE.EXEC                  00060000
